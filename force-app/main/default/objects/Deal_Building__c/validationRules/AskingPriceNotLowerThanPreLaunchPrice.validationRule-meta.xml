<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AskingPriceNotLowerThanPreLaunchPrice</fullName>
    <active>true</active>
    <errorConditionFormula>AND (  Total_Selling_Price_SAR__c &lt;  PreLaunchPrice__c )</errorConditionFormula>
    <errorMessage>Total Selling Price cannot be lower than Pre Launch Price.</errorMessage>
</ValidationRule>
